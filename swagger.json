"/umma": {
  "get": {
    "summary": "Get media content from a URL using Umma",
    "tags": ["Umma"],
    "parameters": [
      {
        "in": "query",
        "name": "url",
        "schema": {
          "type": "string"
        },
        "required": true,
        "description": "URL of the media content"
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "description": "Title of the media content"
                },
                "author": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Name of the author"
                    },
                    "profilePic": {
                      "type": "string",
                      "format": "uri",
                      "description": "URL of the author's profile picture"
                    }
                  }
                },
                "caption": {
                  "type": "string",
                  "description": "Caption of the media content"
                },
                "media": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "format": "uri",
                    "description": "URL of the media content"
                  }
                },
                "type": {
                  "type": "string",
                  "description": "Type of the media content (image or video)"
                },
                "like": {
                  "type": "string",
                  "description": "Number of likes on the media content"
                }
              }
            }
          }
        }
      }
    }
  }
},
